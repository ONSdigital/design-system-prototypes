{% from "components/button/_macro.njk" import onsButton %}
{% from "components/external-link/_macro.njk" import onsExternalLink %}
{% from "components/lists/_macro.njk" import onsList %}

{% set pageConfig = {
    "header": {
        "logoHref": '/',
        "titleLogoHref": pageInfo.rootPath + '#0'
    },
    "title": "Website search example",
    "footer": {
        "OGLLink": {
            "pre": 'All content is available under the',
            "link": 'Open Government Licence v3.0',
            "url": 'https://www.nationalarchives.gov.uk/doc/open-government-licence/version/3/',
            "post": ', except where otherwise stated'
        }
    },
    "breadcrumbs": {
        "itemsList": [
            {
                "text": 'Prototypes',
                "url": '/'
            }
        ]
    }
} %}

{% block main %}
    <h1>Website search prototype</h1>

    <p>This prototype displays the search component as used to search the ONS Design System website.</p>

    {{
        onsButton({
            "text": "View prototype",
            "classes": "ons-u-mt-s",
            "url": 'https://ons-design-system.netlify.app/'
        })
    }}

    <script>
        sessionStorage.clear()
    </script>

    <link rel="stylesheet" href="/prototypes/dac-may2022/styles.css" />
{% endblock %}
