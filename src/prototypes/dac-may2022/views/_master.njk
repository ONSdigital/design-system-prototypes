{% extends "../layout/_template.njk" %}

{% set pageConfig = {
    "header": {
        "logoHref": '/',
        "titleLogoHref": pageInfo.rootPath + '#0'
    },
    "title": pageInfo.title if pageInfo.title else "DAC audit prototype - May 2022",
    "footer": {
        "OGLLink": {
            "pre": 'All content is available under the',
            "link": 'Open Government Licence v3.0',
            "url": 'https://www.nationalarchives.gov.uk/doc/open-government-licence/version/3/',
            "post": ', except where otherwise stated'
        }
    },
    "breadcrumbs": {
        "ariaLabel": 'Breadcrumbs',
        "itemsList": [
            {
                "linkClasses": 'js-previous' if prevPage else '',
                "url": pageInfo.rootPath + "/" + prevPage + '.html',
                "text": 'Back'
            }
        ]
    },
    "mainCol": {
        "columns": pageInfo.columns if pageInfo.columns else '8'
    }
} %}

{% block head %}
    <link rel="stylesheet" href="/prototypes/dac-may2022/styles.css" />
{% endblock %}

{% block preMain %}
{% endblock %}

{% block main %}
{% endblock %}

{% block bodyEnd %}
    <script src="/prototypes/dac-may2022/index.js"></script>
{% endblock %}
